
public class UserManager {
	
	public void Add(User user) 
	{
		System.out.println("id :"+user.id
							+"\nAd :"+user.Name
							+"\nSoyad :"+user.Surname
							+"\nTckn :"+user.identityNumber
							);
	}
	
	public void AddMultiple(User[] users) 
	{
		 for (User user : users) {
			Add(user);
		}
	}
	
}
